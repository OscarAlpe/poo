Algoritmo Hoja1Ejercicio8
	// VIII - Selección
	num1 <- 0
	num2 <- 0
	Escribir "Introduce número 1"
	Leer num1
	Escribir "Introduce número 2"
	Leer num2
	Si num1 > num2 Entonces
		Escribir num1
	SiNo
		Si num1 = num2 Entonces
			Escribir "Son iguales"
		SiNo
			Escribir num2
		FinSi
	FinSi
FinAlgoritmo

