Algoritmo Hoja1Ejercicio9
	// IX - Operadores
	num1 <- 0
	num2 <- 0
	suma <- 0
	resta <- 0
	producto <- 0
	division <- 0
	Escribir "Introduce número 1"
	Leer num1
	Escribir "Introduce número 2"
	Leer num2
	suma = num1 + num2
	resta = num1 -num2
